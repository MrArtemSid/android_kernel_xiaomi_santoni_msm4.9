/dts-v1/;

#include "msm8940.dtsi"
#include "pmi8950.dtsi"
#include "xiaomi/landtoni/common/msm8937-qrd.dtsi"
#include "msm8937-mdss-panels.dtsi"
#include "msm8940-pinctrl.dtsi"

#include "xiaomi/xiaomi.dtsi"
#include "xiaomi/landtoni/common/msm8937-common.dtsi"
#include "xiaomi/landtoni/santoni/msm8937-S88536AA2.dtsi"

/ {
	model = "Qualcomm Technologies, Inc. MSM8940-PMI8950 QRD SKU7";
	compatible = "qcom,msm8940-qrd-sku7", "qcom,msm8940-qrd",
			"qcom,msm8940", "qcom,qrd";
	qcom,board-id= <0x1000b 1>, <0x2000b 1>;
	qcom,pmic-id = <0x10019 0x010011 0x0 0x0>;

	xiaomi,series = "landtoni";
	xiaomi,device = "santoni";
};

/ {
	/delete-node/ qcom,battery-data;
	qrd_batterydata: qcom,battery-data {
		qcom,batt-id-range-pct = <15>;
		#include "xiaomi/landtoni/santoni/batterydata-qrd-Desay-4v4-4100mah.dtsi"
		#include "xiaomi/landtoni/santoni/batterydata-qrd-Atl-4v4-4100mah.dtsi"
		#include "xiaomi/landtoni/santoni/batterydata-qrd-Coslight-4v4-4100mah.dtsi"
		#include "xiaomi/landtoni/santoni/batterydata-qrd-Default-4v4-4100mah.dtsi"
	};
};
